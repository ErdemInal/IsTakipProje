{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.4.0",
      "destination": "wwwroot/lib/twitter-bootstrap/css/twitter-bootstrap/"
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [ "jquery.min.js" ]
    },
    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jqueryValidate",
      "files": [ "jquery.validate.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jqueryUnob",
      "files": [ "jquery.validate.unobtrusive.min.js" ]
    },
    {
      "library": "font-awesome@6.1.1",
      "destination": "wwwroot/lib/fontAwesome",
      "files": ["css/all.min.css","js/all.min.js","webfonts/fa-solid-900.ttf","webfonts/fa-solid-900.woff2"]
    }
  ]
}